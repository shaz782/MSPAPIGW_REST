<?xml version="1.0"?>
<jxb:bindings version="1.0"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
    xmlns:xs="http://www.w3.org/2001/XMLSchema">
	
	<jxb:bindings schemaLocation="CommonData.xsd" node="/xs:schema">
		<!-- 
		 if generateElementProperty="false", it will not generated JAXBElement<ClassType> if nillable property = 'true' also.
		 -->
	    <jxb:globalBindings xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema" generateElementProperty="false">
			<!--
				Here adapter="Adapter qualified name", this we need to create prior to build execution, 
				this will not be generated by cxfbuild
			-->
			<xjc:javaType name="java.util.Calendar" xmlType="xs:dateTime"
				adapter="com.xius.xbus.messages.common.CalendarAdapter"/>
	    </jxb:globalBindings>
	</jxb:bindings>
	
</jxb:bindings>

